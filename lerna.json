{
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "conventionalCommits": true,
      "npmClient": "npm",
      "noGitReset": true,
      "allowBranch": ["*"]
    },
    "bootstrap": {
      "loglevel": "info",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "0.4.8"
}
