{
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "conventionalCommits": true,
      "npmClient": "npm",
      "noGitReset": true,
      "allowBranch": [
        "*"
      ]
    },
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): publish %s",
      "signGitCommit": true,
      "signGitTag": true,
      "noGitTagVersion": true,
      "noPush": true
    },
    "bootstrap": {
      "loglevel": "info",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "0.4.10"
}
