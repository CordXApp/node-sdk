{
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "conventionalCommits": true,
      "npmClient": "npm"
    },
    "version": {
      "conventionalCommits": true,
      "allowBranch": "master",
      "message": "publish new version",
      "npmClient": "npm",
      "exact": true
    },
    "bootstrap": {
      "loglevel": "info",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "0.4.2"
}
