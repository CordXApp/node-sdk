{
    "command": {
        "publish": {
            "registry": "https://registry.npmjs.org/",
            "conventionalCommits": true,
            "npmClient": "npm",
            "noGitReset": true,
            "allowBranch": ["*"]
        },
        "version": {
            "conventionalCommits": true,
            "message": "chore(release): publish %s",
            "forcePublish": ["*"],
            "noPush": true
        },
        "bootstrap": {
            "loglevel": "info",
            "npmClientArgs": ["--no-package-lock"]
        }
    },
    "packages": ["packages/*"],
    "version": "0.4.13"
}
