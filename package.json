{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/busboy": "^0.2.3",
    "@types/lodash": "^4.14.165",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "axios": "^1.4.0",
    "chalk": "3.0.0",
    "eslint": "^8.47.0",
    "busboy": "^0.3.1",
    "fastify": "^3.8.0",
    "fastify-plugin": "^3.0.0",
    "form-data": "^3.0.0",
    "form-urlencoded": "^4.2.1",
    "lodash": "^4.17.20",
    "lerna": "^7.1.5",
    "moment": "^2.30.1",
    "node-fetch": "^2.6.1",
    "ora": "4.0.0",
    "prettier": "^3.0.2",
    "raw-body": "^2.5.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.2",
    "undici": "^5.21.0"
  },
  "peerDependencies": {
    "@types/node": ">= 18.15.5",
    "typescript": ">= 5.0.2"
  },
  "scripts": {
    "build": "lerna run build ",
    "bootstrap": "lerna exec npm install",
    "docs:gen": "typedoc",
    "format": "prettier -w ./packages/**/src/**/*.{ts,tsx,js,jsx}",
    "linter": "eslint --fix ./packages/**/src/**/*.{ts,tsx,js,jsx}",
    "patch": "lerna publish prerelease",
    "publish": "lerna publish --yes && git add . && git commit -m 'chore: version bump and changelog update'",
    "test": "lerna run test",
    "validate": "prettier -c ./packages/**/src/**/*.{ts,tsx,js,jsx}",
    "version": "lerna version --no-git-reset --yes"
  },
  "homepage": "https://cordxapp.github.io/node-sdk/index.html",
  "author": {
    "name": "Toxic Dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CordXApp/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/CordXApp/node-sdk/issues"
  }
}